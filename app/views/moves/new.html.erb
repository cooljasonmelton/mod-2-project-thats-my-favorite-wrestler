<h1> Add a Wrestling Move to the Website </h1>

<%if @errors%>
    <%@errors.each do |error|%>
        <p><%=error%></p>
    <%end%>
<%end%>

<%= form_for @move do |f| %>
    <%= f.label :name, 'Move Name:' %><br>
    <%= f.text_field :name %><br>
    <%= f.label :description, 'Description:' %><br>
    <%= f.text_area :description %><br>
    <%= f.label :img_url, 'Image URL:' %><br>
    <%= f.text_field :img_url %><br>
    <%= f.submit %>
<% end %>

<p><%= link_to "Back to Wrestling Moves", moves_path %> </p>
