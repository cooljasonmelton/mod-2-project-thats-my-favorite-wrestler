<h1> Edit Wrestling Move </h1>

<%= form_for @move do |f| %>
    <%= f.label :name, 'Move Name:' %><br>
    <%= f.text_field :name %><br>
    <%= f.label :description, 'Description:' %><br>
    <%= f.text_area :description %><br>
    <%= f.label :img_url, 'Image URL:' %><br>
    <%= f.text_field :img_url %><br>
    <%= f.submit %>
<% end %>