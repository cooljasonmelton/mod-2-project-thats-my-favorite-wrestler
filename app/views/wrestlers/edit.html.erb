<h1> Edit <%= @wrestler.name %></h1>

<%= form_for @wrestler do |f| %>
    <%= f.label :name, 'Wrestler Name:' %><br>
    <%= f.text_field :name %><br>
    <%= f.label :bio, 'Bio:' %><br>
    <%= f.text_area :bio %><br>
    <%= f.label :img_url, 'Image URL:' %><br>
    <%= f.text_field :img_url %><br>
    <%= f.submit %>
<% end %>

<p><%= link_to "Back to Wrestler Profiles", wrestlers_path %> </p>
