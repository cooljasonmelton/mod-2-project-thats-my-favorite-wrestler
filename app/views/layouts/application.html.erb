<!DOCTYPE html>
<html>
  <head>
    <title>That's My Favorite Wrestler</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.3.3/dist/semantic.min.css">
  <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.3.3/dist/semantic.min.js"></script>
  <link rel="stylesheet" href="styles.css">
  </head>

  <body class="center">
  <div class="ui inverted segment">
    <div class="ui inverted secondary menu">
      <a class="item" href="/users"> 
        User Pages 
      </a>
      <a class="item" href="/wrestlers">
        Wrestler Pages
      </a>
      <a class="item" href="/moves">
        Move Pages
      </a>
      <a class="item" href="/favorite_wrestlers">
        Favorite Wrestler Feed
      </a>
      <a class="item" href="/favorite_moves">
        Favorite Move Feed
      </a>
      <a class="item" href="/users/<%=session[:user_id]%>">
        My User Page
      </a>
    </div>
  </div>
  <div style="float:right">
      <%= button_to "Sign Out", logout_path, method: :delete, class: 'ui button'  %>
  </div>
  

  

<div class="pages">

    <%= yield %>

</div> 

  </body>
</html>
